	/**
	 * This POM method will Add a Product From Sharks In Checkout
	 */
	public void addAProdInSharks() {
		wait = new WebDriverWait(driver, 1);
		objGenerator = new Random();

		if (!flagModalAlerta) {

			try {
				if (RappiUtility.verifyAnElement(true, driver.findElement(titleSharks))
						&& RappiUtility.verifyAnElement(false, driver.findElement(infoGralProdInSharks))
						&& RappiUtility.verifyAnElement(false, driver.findElement(btnCloseSharks))
						/*&& RappiUtility.verifyAnElement(false, driver.findElement(btnPreviousSharks))
						&& RappiUtility.verifyAnElement(false, driver.findElement(btnNextSharks))*/) {

					List<WebElement> elemInSharks = driver.findElements(listProdInSharks);
					int numProdSharks = elemInSharks.size();
					// System.out.println("Num Prod Sharks: " +numProdSharks);
							
					if (numProdSharks < 1) {
						RappiUtility.takeScreen("Img_Error_" + sTestCaseName + "/ProductsInSharks_Empty");
						throw new IOException("ERROR #PRODUCTS SHARKS: Products Number In Sharks Invalid < 1!");
					} else {
						int randProdSharks = objGenerator.nextInt(numProdSharks);
						// System.out.println("Num Rand Sharks: " +randProdSharks);
						
						RappiUtility.scrollToElement(elemInSharks.get(randProdSharks));
						assertTrue("ERROR PRODUCT SHARKS: Product Selected In Sharks isn't Displayed!",
								RappiUtility.verifyAnElement(true, elemInSharks.get(randProdSharks)));

						RappiUtility.takeScreen(sTestCaseName + "/Img_ProductSelected_InSharks");

						String strNameProdSharks = elemInSharks.get(randProdSharks).findElement(By.cssSelector("p"))
								.getAttribute("title").toString();
						System.out.println("Name Prod Sharks:" + strNameProdSharks);

						elemInSharks.get(randProdSharks).findElement(btnAddProdInSharks).click();
						RappiUtility.verifyAnElement(true, driver.findElement(btnCloseSharks));
						driver.findElement(btnCloseSharks).click();
					}

				} else {
					throw new IOException("ERROR CONTENT SHARKS: Some Content in 'Sharks' isn't Displayed!");
				}
				// Old Checkout
//					wait.until(ExpectedConditions.visibilityOfElementLocated(infoGralPayTarjCredit));
//					if (RappiUtility.verifyAnElement(true, driver.findElement(infoGralPayTarjCredit))
//							&& RappiUtility.verifyAnElement(true, driver.findElement(btnAddCredCardInCheckout))) {
//
//						driver.findElement(btnAddCredCardInCheckout).click();
//					} else {
//						throw new IOException("ERROR INFO CREDIT-CARD: Some Content in 'Credit Card' isn't Displayed!");
//					}
			} catch (Exception e) {
				System.out.println(e.getMessage());
			}
		}
	}
	
	/**
	 * This POM method will validate Discount for Prime User in New Checkout
	 */
	public void verifyDiscountPrimeInCheckout() {
		String conditionPrime = "";
		int intConditionPrime = 0;

		if (!flagModalAlerta) {
			try {
				driver.findElement(avisoPrimeUser);
				wait.until(ExpectedConditions.visibilityOfElementLocated(avisoPrimeUser));
				if (RappiUtility.verifyAnElement(true, driver.findElement(avisoPrimeUser))) {
					System.out.println("Usuario Prime");

					conditionPrime = driver.findElement(avisoPrimeUser).getText().replaceAll("[^\\d]", "");
					intConditionPrime = Integer.parseInt(conditionPrime);
					System.out.println("Cond Prime:" + conditionPrime);

					if (checkPricesForPrime(intConditionPrime)) {
						if (selectCreditCardAndApplyPrime()) {
							RappiUtility.takeScreen(sTestCaseName + "/Img_PrimeDiscountApplied_Correct");
						} else {
							RappiUtility.takeScreen("Img_Error_" + sTestCaseName + "/PrimeDiscount_Failed");
							System.out.println("No es posible aplicar Prime");
						}
					} else {
						System.out.println("Precio Prod no cumple condicion Prime");
						RappiUtility.takeScreen(sTestCaseName + "/Img_CheckPricesForApplyPrime_Invalid");
					}
				}
			} catch (Exception e) {
				System.out.println("Usuario no es Prime");
				RappiUtility.takeScreen(sTestCaseName + "/Img_IsNotPrimeUser");
			}
		}
	}
	
	/**
	 * This POM method will Check Prices In Checkout for Prime User
	 * 
	 * @return TRUE only if Prices for Prime User are Corrects
	 */
	public Boolean checkPricesForPrime(int conditPrime) {
		String strPriceProd = "";
		int intPriceProd = 0;

		try {
			System.out.println("Try_checkPricesForPrime");
			
			strPriceProd = getDetailPriceInSummaryPrice("Products");
			// System.out.println("Str Price Prod: " + strPriceProd);
			intPriceProd = Integer.parseInt(strPriceProd);
			// System.out.println("int Price Prod: " + intPriceProd);

			if (intPriceProd >= conditPrime) {
				System.out.println("Precio Prod cumple condicion Prime");
				return true;
			} else {
				throw new IOException("ERROR PRICES: Product Price Minor to Condition Prime!");
			}
		} catch (Exception e) {
			System.out.println("Catch_checkPricesForPrime_" + sBrowserName);
			System.out.println(e.getMessage());
			return false;
		}
	}
	
	/**
	 * This POM method will Select Credit Card Option and 
	 * Check Condition Prime In Checkout for Prime User
	 * 
	 * @return TRUE only if Check for Prime User is Correct
	 */
	public Boolean selectCreditCardAndApplyPrime() {
		String strTotalPrice, strDeliveryPrice, strDeliveryPriceAfter = "";
		int intTotalPrice, intDeliveryPrice = 0;

		try {
			System.out.println("Try_selectCreditCardAndApplyPrime");

			strTotalPrice = driver.findElement(textPrecioTotal).getText().replaceAll("[^\\d]", "");
			intTotalPrice = Integer.parseInt(strTotalPrice);
			System.out.println("int Total Price: " + intTotalPrice);

			strDeliveryPrice = getDetailPriceInSummaryPrice("Delivery");
			// System.out.println("Str Delivery: " + strDeliveryPrice);
			int tamStr = strDeliveryPrice.length();
			// System.out.println("Tam Str Delivery: " + tamStr);

			if (!strDeliveryPrice.isEmpty()) {
				intDeliveryPrice = Integer.parseInt(strDeliveryPrice);
				System.out.println("Int Delivery: " + intDeliveryPrice);
				selectAPayMethodInCheckout(0);

				strDeliveryPriceAfter = getDetailPriceInSummaryPrice("Delivery");
				// System.out.println("Str Delivery2: " + strDeliveryPriceAfter);
				String strDeliveryNew = strDeliveryPriceAfter.substring(tamStr);
				// System.out.println("New Delivery: " + strDeliveryNew);

				int intNewTotalPrices = intTotalPrice - intDeliveryPrice;
				// System.out.println("New Total: " + intNewTotalPrices);

				String strTotalPriceAfter = driver.findElement(textPrecioTotal).getText().replaceAll("[^\\d]", "");
				int intTotalPriceAfter = Integer.parseInt(strTotalPriceAfter);
				// System.out.println("New Total After: " + intTotalPriceAfter);

				if (strDeliveryNew.contentEquals("0") && (intNewTotalPrices == intTotalPriceAfter)) {
					System.out.println("Prime Aplicado Exitoso");
					RappiUtility.highLightElement(driver.findElement(infoGralPreciosCheckout), true);			
					return true;
				} else {
					System.out.println("Valor de Domicilio no es 0");
					throw new IOException("ERROR PRIME DELIVERY: Delivery Price isn't 0!");
				}
			} else {
				System.out.println("Lista Precios No contiene Valor de Domicilio");
				throw new IOException("ERROR DELIVERY PRICES: Delivery Price isn't Displayed!");
			}
		} catch (Exception e) {
			System.out.println("Catch_selectCreditCardAndApplyPrime_" + sBrowserName);
			System.out.println(e.getMessage());
			return false;
		}
	}
	
	/**
	 * This POM method will Get the Detail Price since Summary Prices
	 * 
	 * @param tipoPrecio String with Detail Price to Get
	 * 
	 * @return String with the Detail price found
	 */
	String getDetailPriceInSummaryPrice(String tipoPrecio) {
		List<WebElement> elemPrices;
		String strPriceFound = "";

		try {
			elemPrices = driver.findElements(listDescripcionPrecios);

			for (WebElement e : elemPrices) {
				// RappiUtility.highLightElement(e, true);
				if (e.getText().contains(tipoPrecio)) {
					strPriceFound = e.findElement(priceProdInCheckout).getText().replaceAll("[^\\d]", "");
					// System.out.println("Price Found: " + strPriceFound);
					break;
				}
			}

			if (!strPriceFound.isEmpty()) {
				return strPriceFound;
			} else {
				System.out.println("No Existe Valor para '" + tipoPrecio + "'");
				throw new IOException("ERROR SUMMARY PRICES: " + tipoPrecio + " Price isn't Displayed!");
			}

		} catch (Exception e) {
			System.out.println("Catch_searchDetailPrice_" + sBrowserName);
			System.out.println(e.getMessage());
			return null;
		}
	}

	
}
